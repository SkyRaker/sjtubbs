<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
	android:title="@string/setting">
	<PreferenceCategory android:title="@string/setting">
		<CheckBoxPreference android:title="@string/automaticlogin"
			android:defaultValue="false" android:key="autologin" />
		<CheckBoxPreference android:title="@string/setting_tail"
			android:defaultValue="true" android:key="post.tail" />
		<CheckBoxPreference android:title="@string/setting_prefix"
			android:defaultValue="true" android:summaryOn="@string/has"
			android:summaryOff="@string/hasnot" android:key="post.prefix" />
		<CheckBoxPreference android:title="@string/setting_storeimg"
			android:summaryOn="@string/setting_storeimg_on" android:summaryOff="@string/setting_storeimg_off"
			android:defaultValue="false" android:key="store.image" />
		<CheckBoxPreference android:title="@string/setting_quote"
			android:summaryOn="@string/setting_quote_on" android:summaryOff="@string/setting_quote_off"
			android:defaultValue="true" android:key="quote" />
		<com.tyt.bbs.preference.DialogPreference
			android:title="@string/setting_about" android:dialogIcon="@drawable/icon"
			android:dialogTitle="@string/app_about" android:dialogMessage="@string/aboutInfo"
			android:dialogLayout="@layout/dialog_about"
			android:negativeButtonText="@null" />
	</PreferenceCategory>
	<PreferenceScreen android:key="app.feedback"
		android:title="@string/setting_feedback">

		<intent android:action="android.intent.action.SENDTO"
			android:data="mailto:tank.tyt@gmail.com" />
	</PreferenceScreen>
</PreferenceScreen>

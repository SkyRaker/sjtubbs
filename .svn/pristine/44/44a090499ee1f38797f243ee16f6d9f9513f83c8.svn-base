<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:background="@drawable/bg_dialog"
>
  	<AutoCompleteTextView android:layout_width="fill_parent"
			android:gravity="center_horizontal" android:layout_height="wrap_content"
			android:id="@+id/autoCT_addFavor" />
		<LinearLayout android:layout_width="fill_parent"
			android:id="@+id/layout_btn" android:layout_height="wrap_content"
			android:layout_below="@+id/autoCT_addFavor">
			<Button android:id="@+id/btn_addFavorok" android:text="@string/ok"
				android:layout_weight="1" android:layout_width="fill_parent"
				android:layout_height="wrap_content" />
			<Button android:id="@+id/btn_addFavorcancel" android:text="@string/cancel"
				android:layout_weight="1" android:layout_width="fill_parent"
				android:layout_height="wrap_content" />
		</LinearLayout>
</RelativeLayout>

<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<solid android:color="#800069b0" />
			<corners 
				android:topLeftRadius="32dip"
				android:bottomLeftRadius="32dip"
				android:topRightRadius="32dip"
				android:bottomRightRadius="32dip" />
			<stroke android:width="1.0dip" android:color="#337fd4" />
			<padding android:left="2dip" android:right="2dip"
				android:top="2dip" android:bottom="2dip" />
		</shape>
	</item>

	<item android:state_focused="true">
		<shape>
			<solid android:color="#804c466c" />
			<corners 
				android:topLeftRadius="32dip"
				android:bottomLeftRadius="32dip"
				android:topRightRadius="32dip"
				android:bottomRightRadius="32dip" />
			<padding android:left="2dip" android:right="2dip"
				android:top="2dip" android:bottom="2dip" />
		</shape>
	</item>
	<item>
		<shape>
			<solid android:color="#00000000" />
			<corners 
				android:topLeftRadius="32dip"
				android:bottomLeftRadius="32dip"
				android:topRightRadius="32dip"
				android:bottomRightRadius="32dip" />
			<padding android:left="2dip" android:right="2dip"
				android:top="2dip" android:bottom="2dip" />
		</shape>
	</item>
</selector>
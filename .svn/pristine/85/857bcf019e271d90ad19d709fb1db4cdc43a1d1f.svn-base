<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- 背景灰色部分 -->
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="12dip" />
			<gradient android:startColor="#454545" android:centerColor="#454545"
				android:centerY="0.50" android:endColor="#454545" android:angle="270" />
		</shape>
	</item>

	<!-- secondary 部分-->
	<item android:id="@android:id/secondaryProgress">
		<clip android:clipOrientation="horizontal" android:gravity="left">
			<shape>
				<stroke android:width="1dip" android:color="#00000000" />
				<corners android:radius="12dip" />
				<gradient android:startColor="#5996B3" android:centerY="0.50"
					android:endColor="#5996B3" android:angle="270" />
			</shape>
		</clip>
		
	</item>

	<!-- 主进度条 -->
	<item android:id="@android:id/progress">

	<clip android:clipOrientation="horizontal" android:gravity="left">
		<shape>
			<stroke android:width="1dip" android:color="#00000000" />
			<corners android:radius="12dip" />
			<gradient android:startColor="#00CDFF" android:centerY="0.50"
				android:endColor="#00A2EE" android:angle="270" />
		</shape>
	</clip>

</item>
</layer-list>

